<odoo>
    <record id="student_management_print_report" model="ir.actions.report">
        <field name="name">Student Report</field>
        <field name="model">student.management</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">student_management.student_report2_template</field>
